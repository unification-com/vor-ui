REACT_APP_API_URL=ws://localhost:8545

# Rinkeby Account (rinkeyby url, mnemonic)
WALLET_URL=http://172.25.0.6:8545
WALLET_KEY=
ETHERSCAN_API=

# DB
DB_HOST=172.25.0.4
DB_NAME=vorui
DB_USER=vorui
